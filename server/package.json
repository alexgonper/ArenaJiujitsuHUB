{
  "name": "arena-matrix-backend",
  "version": "1.0.0",
  "description": "Backend API for Arena Matrix - Jiu-Jitsu Academy Management System",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "seed": "node scripts/seedDatabase.js",
    "setup": "npm install && node scripts/setup.js",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop arena-hub-api",
    "pm2:restart": "pm2 restart arena-hub-api",
    "pm2:logs": "pm2 logs arena-hub-api",
    "pm2:monit": "pm2 monit",
    "optimize:db": "node scripts/optimizeDatabase.js",
    "production": "NODE_ENV=production pm2 start ecosystem.config.js --env production"
  },
  "keywords": [
    "arena",
    "jiu-jitsu",
    "management",
    "api",
    "mongodb"
  ],
  "author": "Arena Jiu-Jitsu",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^3.0.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.3",
    "mercadopago": "^2.11.0",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^7.1.0",
    "nodemon": "^3.0.2",
    "tailwindcss": "^4.1.18"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
